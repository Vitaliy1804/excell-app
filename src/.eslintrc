{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "require-jsdoc":"off",
        "indent": "off",
        "operator-linebreak": "off",
        "no-trailing-spaces":"off",
        "linebreak-style": 0,
        "object-curly-spacing": "off",
        
        
    },
    "extends": ["eslint:recommended", "google"]
    }